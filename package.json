  },
  "homepage": "https://github.com/squarezw/abtest-core#readme",
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@babel/preset-env": "7.9.6",
    "@babel/preset-react": "7.9.4",
    "babel-loader": "8.1.0",
    "chai": "4.2.0",
    "coveralls": "3.1.0",
    "eslint": "7.2.0",
    "eslint-plugin-react": "7.20.0",
    "istanbul": "0.4.5",
    "mocha": "8.0.1",
