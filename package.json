{
  "name": "abtest-core",
  "version": "0.0.1",
  "description": "A ABTesting Core Library",
  "main": "index.js",
  "scripts": {
    "test": "npm run build && mocha -t 5000",
    "postinstall": "cp .hooks/* .git/hooks/",
    "lint": "eslint --fix lib",
    "build": "babel lib -d dist",
    "cover": "istanbul cover node_modules/mocha/bin/_mocha",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/squarezw/abtest-core.git"
  },
  "keywords": [
    "abtest"
  ],
  "author": "jesse zhao",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/squarezw/abtest-core/issues"
  },
  "homepage": "https://github.com/squarezw/abtest-core#readme",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "chai": "4.2.0",
    "coveralls": "3.1.0",
    "eslint": "7.2.0",
    "eslint-plugin-react": "7.20.0",
    "istanbul": "0.4.5",
    "mocha": "8.0.1"
  }
}
